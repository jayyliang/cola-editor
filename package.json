    "@electron-toolkit/utils": "^3.0.0",
    "@octokit/rest": "^20.0.2",
    "antd": "^5.14.1",
    "axios": "^1.6.7",
    "bytemd": "^1.21.0",
    "electron-updater": "^6.1.7",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2",
    "react-router": "^6.22.1",
    "react-router-dom": "^6.22.1"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
